  <credit page="1">
    <credit-words default-x="363" default-y="1287" font-size="19" valign="top">599. А в полі береза (14-БР-04)</credit-words>
  </credit>
  <credit page="1">
    <credit-words default-x="370" default-y="1244" valign="top">60</credit-words>
