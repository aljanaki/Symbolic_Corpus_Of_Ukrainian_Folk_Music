  <credit page="1">
    <credit-words default-x="330" default-y="1264" font-size="19" valign="top">629. Ой піду я в ліс по дрова (13-БР-06)</credit-words>
  </credit>
  <credit page="1">
    <credit-words default-x="468" default-y="1211" valign="top">60 Повільно</credit-words>
  </credit>
  <credit page="1">
