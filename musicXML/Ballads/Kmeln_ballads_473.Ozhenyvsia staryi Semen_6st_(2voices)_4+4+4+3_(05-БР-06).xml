  <credit page="1">
    <credit-words default-x="276" default-y="1220" font-size="19" valign="top">473. Оженився старий Семен (05-БР-06)</credit-words>
  </credit>
  <credit page="1">
    <credit-words default-x="244" default-y="1189" valign="top">Помірно</credit-words>
  </credit>
  <credit page="1">
