  <credit page="1">
    <credit-words default-x="321" default-y="1216" font-size="19" valign="top">605. Ой за гаєм зелененьким (15-БР-05)</credit-words>
  </credit>
  <credit page="1">
    <credit-words default-x="323" default-y="1176" valign="top">56</credit-words>
  </credit>
  <credit page="1">
