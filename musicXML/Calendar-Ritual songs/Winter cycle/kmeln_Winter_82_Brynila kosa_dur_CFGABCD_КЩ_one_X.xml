  <credit page="1">
    <credit-words default-x="327" default-y="1287" font-size="19" valign="top">82. Бриніла коса (ОК-КЩ-18)</credit-words>
  </credit>
  <credit page="1">
    <credit-words default-x="300" default-y="1232" valign="top">Помірно</credit-words>
