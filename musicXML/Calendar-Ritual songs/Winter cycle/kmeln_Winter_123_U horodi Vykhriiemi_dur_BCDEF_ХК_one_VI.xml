    <credit-words default-x="396" default-y="1265" font-size="19" valign="top">123. У городі Вихріємі (ОК-ХК)</credit-words>
  </credit>
  <credit page="1">
    <credit-words default-x="233" default-y="1220" valign="top">Помірно</credit-words>
  </credit>
  <credit page="1">
