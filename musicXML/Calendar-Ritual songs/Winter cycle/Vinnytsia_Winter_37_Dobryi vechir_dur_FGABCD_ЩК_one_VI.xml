    <credit-words default-x="371" default-y="1208" font-size="19" valign="top">37. Добрий вечір (ОК-ЩК-06)</credit-words>
  </credit>
  <credit page="1">
    <credit-words default-x="244" default-y="1168" valign="top">Скоро</credit-words>
  </credit>
  <credit page="1">
