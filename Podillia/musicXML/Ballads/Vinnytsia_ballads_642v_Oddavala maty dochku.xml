  <credit page="1">
    <credit-words default-x="331" default-y="1260" font-size="19" valign="top">642. Оддавала мати дочку (17-БР-01б)</credit-words>
  </credit>
  <credit page="1">
    <credit-words default-x="403" default-y="1225" valign="top">70 Помірно</credit-words>
  </credit>
  <credit page="1">
