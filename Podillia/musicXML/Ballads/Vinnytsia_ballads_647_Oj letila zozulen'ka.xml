  <credit page="1">
    <credit-words default-x="323" default-y="1268" font-size="19" valign="top">647. Ой летіла зозуленька (19-БР-06)</credit-words>
  </credit>
  <credit page="1">
    <credit-words default-x="455" default-y="1228" valign="top">70 Сумно</credit-words>
  </credit>
  <credit page="1">
