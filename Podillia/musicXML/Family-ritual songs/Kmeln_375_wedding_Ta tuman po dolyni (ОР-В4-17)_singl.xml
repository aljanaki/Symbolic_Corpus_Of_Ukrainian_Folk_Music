    <credit-words default-x="331" default-y="1226" font-size="19" valign="top">375. Та туман по долині (ОР-В4-17)</credit-words>
  </credit>
  <credit page="1">
    <credit-words default-x="280" default-y="1184" valign="top">Помірно</credit-words>
