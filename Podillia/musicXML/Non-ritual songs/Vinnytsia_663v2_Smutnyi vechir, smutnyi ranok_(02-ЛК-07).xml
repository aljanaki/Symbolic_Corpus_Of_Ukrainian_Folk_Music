  <credit page="1">
    <credit-words default-x="249" default-y="1258" font-size="19" valign="top">663. Смутний вечір, смутний ранок (02-ЛК-07)</credit-words>
  </credit>
  <credit page="1">
    <credit-words default-x="100" default-y="1198" valign="top">Вар. 2</credit-words>
  </credit>
  <credit page="1">
