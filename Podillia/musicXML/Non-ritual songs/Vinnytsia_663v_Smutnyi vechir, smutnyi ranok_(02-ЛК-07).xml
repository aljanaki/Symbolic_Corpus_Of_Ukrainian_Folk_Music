  <credit page="1">
    <credit-words default-x="270" default-y="1259" font-size="19" valign="top">663. Смутний вечір, смутний ранок (02-ЛК-07)</credit-words>
  </credit>
  <credit page="1">
    <credit-words default-x="394" default-y="1219" valign="top">54 Повільно</credit-words>
  </credit>
