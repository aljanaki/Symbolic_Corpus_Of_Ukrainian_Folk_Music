  <credit page="1">
    <credit-words default-x="331" default-y="1283" font-size="19" valign="top">816. Ой журавко, журавко (17-ЛР-02) </credit-words>
  </credit>
  <credit page="1">
    <credit-words default-x="479" default-y="1231" valign="top">84 Вільно</credit-words>
  </credit>
  <credit page="1">
