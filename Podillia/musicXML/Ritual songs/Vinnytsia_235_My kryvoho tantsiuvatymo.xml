  <credit page="1">
    <credit-words default-x="310" default-y="1253" font-size="19" valign="top">235. Ми кривого танцюватимо (ОК-ВТ-07)</credit-words>
  </credit>
  <credit page="1">
    <credit-words default-x="487" default-y="1219" valign="top">70 Ходою</credit-words>
