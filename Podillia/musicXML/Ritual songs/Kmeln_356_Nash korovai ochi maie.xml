  <credit page="1">
    <credit-words default-x="293" default-y="706" valign="top">Виробили ми вже коровай, шишки і час сажати коровай у піч.</credit-words>
  </credit>
  <credit page="1">
    <credit-words default-x="343" default-y="683" valign="top">Помірно</credit-words>
