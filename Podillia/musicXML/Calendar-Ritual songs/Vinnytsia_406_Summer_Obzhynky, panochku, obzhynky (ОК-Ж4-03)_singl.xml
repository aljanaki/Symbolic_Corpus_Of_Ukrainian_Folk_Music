  <credit page="1">
    <credit-words default-x="331" default-y="1226" font-size="19" valign="top">406. Обжинки, паночку, обжинки (ОК-Ж4-03)</credit-words>
  </credit>
  <credit page="1">
    <credit-words default-x="433" default-y="1181" valign="top">120 Швиденько</credit-words>
  </credit>
  <credit page="1">
