  <credit page="1">
    <credit-words default-x="275" default-y="786" font-size="19" valign="top">379. Покотю я відеречко на долину (ОК-ПК)</credit-words>
  </credit>
  <credit page="1">
    <credit-words default-x="463" default-y="745" valign="top">96 Помірно</credit-words>
  </credit>
  <credit page="1">
