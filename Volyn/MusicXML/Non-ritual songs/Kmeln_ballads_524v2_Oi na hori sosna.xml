  <credit page="1">
    <credit-words default-x="334" default-y="1287" font-size="18" valign="top">524. Ой на горі сосна (08-БК-04)</credit-words>
  </credit>
  <credit page="1">
    <credit-words default-x="292" default-y="1248" valign="top">Помірно</credit-words>
  </credit>
  <credit page="1">
