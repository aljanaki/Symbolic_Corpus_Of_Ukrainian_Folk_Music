  <credit page="1">
    <credit-words default-x="324" default-y="1270" font-size="19" valign="top">70. Добрий вечір тобі (ОК-КЩ-01б)</credit-words>
  </credit>
  <credit page="1">
    <credit-words default-x="314" default-y="1225" valign="top">90 Помірно</credit-words>
